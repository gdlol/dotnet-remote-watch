{
  "printWidth": 120,
  "plugins": ["prettier-plugin-packagejson", "prettier-plugin-sh", "@prettier/plugin-xml", "prettier-plugin-ini"],
  "xmlWhitespaceSensitivity": "ignore",
  "overrides": [
    { "files": "app.manifest", "options": { "parser": "xml" } },
    { "files": "*.globalconfig", "options": { "parser": "ini" } }
  ]
}
